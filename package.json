{
  "name": "website",
  "version": "1.0.0",
  "description": "Personal Website",
  "author": "Javi Palacios <javi@fjp.es>",
  "private": true,
  "scripts": {
    "start": "node -r esm ./node_modules/.bin/gatsby develop",
    "develop": "node -r esm ./node_modules/.bin/gatsby develop",
    "build": "NODE_ICU_DATA=node_modules/full-icu node -r esm ./node_modules/.bin/gatsby build",
    "serve": "node -r esm ./node_modules/.bin/gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint '*/**/*.{js,json,ts,tsx}'",
    "lint:fix": "eslint '*/**/*.{js,json,ts,tsx}' --quiet --fix",
    "test": "NODE_ICU_DATA=node_modules/full-icu jest --coverage",
    "test:watch": "NODE_ICU_DATA=node_modules/full-icu jest --watch",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cy:run"
  },
  "browserslist": [
    "> 0.25%",
    "last 5 versions",
    "not ie > 0",
    "not ie_mob > 0"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,json,ts,tsx}": [
      "yarn run lint"
    ],
    "*.{scss}": "stylelint"
  },
  "dependencies": {
    "@reach/router": "^1.3.3",
    "gatsby": "^2.24.67",
    "gatsby-plugin-typescript": "^2.4.21",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@mdx-js/mdx": "^1.6.19",
    "@mdx-js/react": "^1.6.19",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^10.4.7",
    "@types/cypress": "^1.1.3",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.14",
    "@types/mdx-js__react": "^1.5.3",
    "@types/react": "^16.9.50",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "autoprefixer": "^9.8.0",
    "babel-jest": "^26.6.1",
    "babel-preset-gatsby": "^0.4.7",
    "cypress": "^5.3.0",
    "cz-conventional-changelog": "3.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.19.0",
    "esm": "^3.2.25",
    "full-icu": "^1.3.1",
    "gatsby-image": "^2.4.21",
    "gatsby-plugin-mdx": "^1.2.51",
    "gatsby-plugin-react-i18next": "^0.0.27",
    "gatsby-plugin-remove-trailing-slashes": "^2.3.13",
    "gatsby-plugin-sass": "^2.3.12",
    "gatsby-plugin-sharp": "^2.7.1",
    "gatsby-plugin-sitemap": "^2.4.17",
    "gatsby-remark-images": "^3.4.1",
    "gatsby-source-filesystem": "^2.3.34",
    "gatsby-transformer-sharp": "^2.5.21",
    "husky": "^4.2.5",
    "i18next": "^19.8.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "jest-transformer-mdx": "^2.2.0",
    "lint-staged": "^10.2.11",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "react-i18next": "^11.7.3",
    "react-test-renderer": "^16.13.1",
    "start-server-and-test": "^1.11.0",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.17.2",
    "stylelint-selector-bem-pattern": "^2.1.0",
    "ts-jest": "^26.4.2",
    "typescript": "^3.9.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fjpalacios/website"
  },
  "bugs": {
    "url": "https://github.com/fjpalacios/website/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
