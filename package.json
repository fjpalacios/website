{
  "name": "website",
  "version": "1.0.0",
  "description": "Personal Website",
  "author": "Javi Palacios <javi@fjp.es>",
  "private": true,
  "scripts": {
    "start": "gatsby develop",
    "develop": "gatsby develop",
    "build": "NODE_ICU_DATA=node_modules/full-icu gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint '*/**/*.{js,json,ts,tsx}'",
    "lint:fix": "eslint '*/**/*.{js,json,ts,tsx}' --quiet --fix",
    "type-check": "tsc",
    "test": "NODE_ICU_DATA=node_modules/full-icu jest --coverage --passWithNoTests",
    "test:watch": "NODE_ICU_DATA=node_modules/full-icu jest --watch"
  },
  "browserslist": [
    "> 0.25%",
    "last 5 versions"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,json,ts,tsx}": [
      "yarn run lint",
      "yarn run test"
    ],
    "*.{ts,tsx}": "yarn type-check --noEmit --jsx preserve --esModuleInterop true --skipLibCheck",
    "*.{scss}": "stylelint"
  },
  "dependencies": {
    "gatsby": "^2.20.18",
    "gatsby-plugin-intl": "^0.3.3",
    "gatsby-plugin-typescript": "^2.3.1",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.2",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/faker": "^4.1.11",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "autoprefixer": "^9.7.5",
    "babel-jest": "^25.3.0",
    "babel-preset-gatsby": "^0.3.1",
    "cz-conventional-changelog": "3.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "faker": "^4.1.0",
    "full-icu": "^1.3.1",
    "gatsby-plugin-sass": "^2.2.1",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.3.0",
    "lint-staged": "^10.1.3",
    "node-sass": "^4.13.1",
    "prettier": "^2.0.4",
    "react-test-renderer": "^16.13.1",
    "stylelint": "^13.3.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "^3.17.0",
    "stylelint-selector-bem-pattern": "^2.1.0",
    "tslint-react": "^4.2.0",
    "typescript": "^3.8.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fjpalacios/website"
  },
  "bugs": {
    "url": "https://github.com/fjpalacios/website/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
