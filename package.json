{
  "name": "website",
  "version": "1.0.0",
  "description": "Personal Website",
  "author": "Javi Palacios <javi@fjp.es>",
  "private": true,
  "scripts": {
    "start": "gatsby develop",
    "develop": "gatsby develop",
    "build": "NODE_ICU_DATA=node_modules/full-icu gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint '*/**/*.{js,json,ts,tsx}'",
    "lint:fix": "eslint '*/**/*.{js,json,ts,tsx}' --quiet --fix",
    "test": "NODE_ICU_DATA=node_modules/full-icu jest --coverage",
    "test:watch": "NODE_ICU_DATA=node_modules/full-icu jest --watch",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cy:run"
  },
  "browserslist": [
    "> 0.25%",
    "last 5 versions"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,json,ts,tsx}": [
      "yarn run lint"
    ],
    "*.{scss}": "stylelint"
  },
  "dependencies": {
    "@reach/router": "^1.3.3",
    "gatsby": "^2.24.27",
    "gatsby-plugin-intl": "^0.3.3",
    "gatsby-plugin-typescript": "^2.4.16",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.4.7",
    "@types/cypress": "^1.1.3",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.2.3",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "autoprefixer": "^9.8.0",
    "babel-jest": "^26.0.1",
    "babel-preset-gatsby": "^0.4.7",
    "cypress": "^5.2.0",
    "cz-conventional-changelog": "3.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "full-icu": "^1.3.1",
    "gatsby-plugin-sass": "^2.3.12",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.11",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1",
    "start-server-and-test": "^1.11.0",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.17.2",
    "stylelint-selector-bem-pattern": "^2.1.0",
    "typescript": "^3.9.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fjpalacios/website"
  },
  "bugs": {
    "url": "https://github.com/fjpalacios/website/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
