{
  "name": "website",
  "version": "1.0.0",
  "description": "Personal Website",
  "author": "Javi Palacios <javi@fjp.es>",
  "private": true,
  "scripts": {
    "start": "gatsby develop",
    "develop": "gatsby develop",
    "build": "NODE_ICU_DATA=node_modules/full-icu gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint '*/**/*.{js,json,ts,tsx}'",
    "lint:fix": "eslint '*/**/*.{js,json,ts,tsx}' --quiet --fix",
    "test": "NODE_ICU_DATA=node_modules/full-icu jest --coverage",
    "test:watch": "NODE_ICU_DATA=node_modules/full-icu jest --watch",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cy:run",
    "prepare": "husky install"
  },
  "browserslist": [
    "> 0.25%",
    "last 5 versions"
  ],
  "lint-staged": {
    "*.{js,json,ts,tsx}": [
      "yarn run lint"
    ],
    "*.scss": "stylelint"
  },
  "dependencies": {
    "@reach/router": "^1.3.4",
    "gatsby": "^5.12.4",
    "gatsby-plugin-intl": "^5.10.0",
    "gatsby-plugin-typescript": "^5.12.0",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/cypress": "^1.1.3",
    "@types/enzyme": "^3.10.13",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^29.5.5",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "autoprefixer": "^10.4.16",
    "babel-jest": "^29.7.0",
    "babel-preset-gatsby": "^3.12.0",
    "cypress": "^13.2.0",
    "cz-conventional-changelog": "^3.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.7",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-jest": "^27.4.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "full-icu": "^1.5.0",
    "gatsby-plugin-sass": "^6.12.0",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^14.0.1",
    "node-sass": "^8.0.0",
    "prettier": "^3.0.3",
    "react-test-renderer": "^18.2.0",
    "start-server-and-test": "^2.0.1",
    "stylelint": "^15.10.3",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^5.2.1",
    "stylelint-selector-bem-pattern": "^3.0.1",
    "typescript": "^5.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fjpalacios/website"
  },
  "bugs": {
    "url": "https://github.com/fjpalacios/website/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
